{
    "name": "curlit",
    "version": "1.0.0",
    "description": "An Express middleware that logs a cURL command representation of incoming requests and responses. You can then copy the curl, run it or paste into postman to build a postman collection.",
    "main": "index.js",
    "scripts": {
        "lint": "standard --fix"
    },
    "standard": {
        "ignore": [
            "**/.trash/*"
        ]
    },
    "keywords": [
        "express",
        "middleware",
        "curl",
        "logging"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daggieblanqx/curlit.git"
    },
    "bugs": {
        "url": "https://github.com/daggieblanqx/curlit/issues"
    },
    "homepage": "https://github.com/daggieblanqx/curlit#readme",
    "author": {
        "name": "daggie blanqx",
        "url": "https://github.com/daggieblanqx"
    },
    "license": "MIT",
    "devDependencies": {
        "standard": "^17.1.2"
    }
}